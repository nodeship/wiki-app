{
  "name": "wiki-app",
  "version": "0.0.0",
  "description": "Random wikipedia App",
  "repository": "https://github.com/nodeship/wiki-app",
  "author": "Nivrith <nodeship1209@gmail.com>",
  "license": "MIT",
  "keywords": [
    "wiki-app"
  ],
  "scripts": {
    "serve": "npx webpack-dev-server --env.mode=development",
    "serve:prod": "npx webpack-dev-server --env.mode=production",
    "build": "npx webpack --env.mode=production",
    "test": "nyc mocha --require ts-node/register test/**/*.spec.ts",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run clean && npm run compile",
    "clean": "del-cli dist",
    "release": "npx np",
    "docs": "typedoc src",
    "devtools": "node --inspect --inspect-brk",
    "benchmark": "tsc -b && node",
    "version": "npx auto-changelog -p && git add CHANGELOG.md"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "handlebars": "^4.5.3",
    "html-loader": "^0.5.5",
    "xeta": "^1.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.2.2",
    "@types/cli-table2": "^0.2.2",
    "@types/mocha": "^5.2.7",
    "auto-changelog": "^1.16.1",
    "awesome-typescript-loader": "^5.2.1",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cli-table2": "^0.2.0",
    "css-loader": "^3.2.0",
    "del-cli": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "fast-memoize": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "logdown": "^3.2.8",
    "microtime": "^3.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ora": "^3.4.0",
    "style-loader": "^1.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
